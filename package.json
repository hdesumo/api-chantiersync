{
  "name": "api-chantiersync",
  "version": "1.0.0",
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon app.js",
    "start": "node app.js",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "9.0.2",
    "pg": "8.12.0",
    "pg-hstore": "2.3.4",
    "qrcode": "^1.5.4",
    "sequelize": "6.37.7",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "nodemon": "3.1.4",
    "sequelize-cli": "6.6.3"
  }
}
